# Revert back to last svn checkin
svn revert --depth=infinity ..

# Allow us access to the raw Vault database
chmod go+rw "../altium_vault_files/Data/EmbFirebirdDb.fdb"

# Fix the permissions on this script itself
chmod +x $0

# Note to user
echo ""
echo "This only did the revert of the local machine's copy of the Vault!"
echo "You need to login to the Vault server and do a revert there!"

